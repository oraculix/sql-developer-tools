	<query>
		<sql><![CDATA[SELECT typ, SUM(gb) GB FROM
(
SELECT 'Datafiles' typ, ROUND( SUM( bytes/1024/1024/1024), 2 ) GB
  FROM dba_data_files
UNION
SELECT 'Tempfiles' typ, ROUND( SUM( bytes/1024/1024/1024), 2 ) GB
  FROM dba_temp_files
UNION
SELECT 'Logfiles' typ, ROUND( SUM( bytes*members/1024/1024/1024), 2 ) GB
  FROM v$log
)
GROUP BY ROLLUP(typ)]]></sql>
	</query>
